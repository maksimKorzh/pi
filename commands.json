{
  "quit": {
    "bind": "((ord('z')) & 0x1f) == ch",
    "exec": "sys.exit()"
  },
  "save": {
    "bind": "((ord('s')) & 0x1f) == ch",
    "exec": "cont=''\nfor l in b[:-1]: cont += ''.join([chr(c) for c in l]) + '\\n'\nwith open(src, 'w') as f: f.write(cont)"
  },
  "home": {
     "bind": "curses.KEY_HOME == ch",
     "exec": "c=0"
  },
  "end": {
     "bind": "curses.KEY_END == ch",
     "exec": "c=len(b[r])"
  }
}
